<!-- ARM/GRIPPER -->
<!--             -->
<?xml version="1.0"?>
<sdf version="1.4">
<model name="arm">
	<plugin name="ArmPlugin" filename="libgazeboArmPlugin.so" />
	<static>false</static>

	<link name="base">
	<pose>0 0 .1 0 0 0</pose>
	<inertial>
		<pose>0 0 0 0 0 0</pose>
		<inertia>
			<ixx>0.01</ixx>
			<ixy>0</ixy>
			<ixz>0</ixz>
			<iyy>0.01</iyy>
			<iyz>0</iyz>
			<izz>0.01</izz>
		</inertia>
		<mass>10</mass>
	</inertial>
	<collision name="collision">
		<geometry>
			<box>
				<size>0.4 0.4 0.2</size>
			</box>
		</geometry>
	</collision>
	<visual name="visual">
		<geometry>
			<box>
				<size>0.4 0.4 0.2</size>
			</box>
		</geometry>
	</visual>
	</link>

	<link name="joint1">
	<pose>0 0 0.3 0 1.5707 1.5707</pose>
	<collision name="collision">
		<geometry>
			<cylinder>
				<radius>.1</radius>
				<length>.15</length>
			</cylinder>
		</geometry>
	</collision>
	<visual name="visual">
		<geometry>
			<cylinder>
				<radius>.1</radius>
				<length>.15</length>
			</cylinder>
		</geometry>
	</visual>
	</link>

	<link name="link1">
	<pose>0 0 .66 0 0 0</pose>
	<inertial>
		<mass>.25</mass>
		<inertia>
			<!-- interias are tricky to compute -->
			<ixx>0.083</ixx>
			<!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
			<ixy>0.0</ixy>
			<!-- for a box: ixy = 0 -->
			<ixz>0.0</ixz>
			<!-- for a box: ixz = 0 -->
			<iyy>0.083</iyy>
			<!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
			<iyz>0.0</iyz>
			<!-- for a box: iyz = 0 -->
			<izz>0.083</izz>
			<!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
		</inertia>
	</inertial>
	<collision name="collision">
		<geometry>
			<box>
				<size>.1 .1 .6</size>
			</box>
		</geometry>
	</collision>
	<visual name="visual">
		<geometry>
			<box>
				<size>.1 .1 .6</size>
			</box>
		</geometry>
	</visual>
	</link>


	<link name="joint2">
	<pose>0 0 1.01 0 1.5707 1.5707</pose>
	<collision name="collision">
		<geometry>
			<cylinder>
				<radius>.1</radius>
				<length>.15</length>
			</cylinder>
		</geometry>
	</collision>
	<visual name="visual">
		<geometry>
			<cylinder>
				<radius>.1</radius>
				<length>.15</length>
			</cylinder>
		</geometry>
	</visual>
	</link>

	<link name="link2">
	<pose>0 0 1.37 0 0 0</pose>
	<inertial>
		<mass>.2</mass>
		<inertia>
			<ixx>0.083</ixx>
			<ixy>0.0</ixy>
			<ixz>0.0</ixz>
			<iyy>0.083</iyy>
			<iyz>0.0</iyz>
			<izz>0.083</izz>
		</inertia>
	</inertial>
	<collision name="collision2">
		<geometry>
			<box>
				<size>.1 .1 .6</size>
			</box>
		</geometry>
	</collision>
	<visual name="visual2">
		<geometry>
			<box>
				<size>.1 .1 .6</size>
			</box>
		</geometry>
	</visual>
	</link>
	
	<link name="gripperbase">
		<pose>0 0 1.68 0 0 0</pose>
		<inertial>
			<mass>0.3</mass>
			<inertia>
			  <ixx>0.0001</ixx>
			  <ixy>0</ixy>
			  <ixz>0</ixz>
			  <iyy>0.0002</iyy>
			  <iyz>0</iyz>
			  <izz>0.0001</izz>
			</inertia>
		</inertial>
		<visual name="gripper_link">		   
			<geometry>
				<mesh><uri>file://meshes/gripper/gripper_base.dae</uri></mesh>
			</geometry>
		</visual>
		<collision name="gripper_link">    
			<geometry>
				<mesh><uri>file://meshes/gripper/gripper_base.dae</uri></mesh>
			</geometry>
		</collision>
	</link>

	<link name="gripper_middle">
		<pose>0 0 1.77 0 0 0</pose>
		<collision name="middle_collision">
		<geometry>
			<box>
				<size>.01 .01 .07</size>
			</box>
		</geometry>
		</collision>              
		<visual name="middle_vision">
			<geometry>
				<box>
					<size>.01 .01 .07</size>
				</box>
			</geometry>
		</visual>
	</link>

	<link name="gripper_right">
		<pose>0 0.075 1.805 0 -1.57 -3.14</pose>
		<inertial>
			<mass>0.1</mass>
			<inertia>
				<ixx>0.02</ixx>
				<ixy>0</ixy>
				<ixz>0</ixz>
				<iyy>0.0002</iyy>
				<iyz>0</iyz>
				<izz>0.0002</izz>
			</inertia>
		</inertial>
		<visual name="right_gripper">
			<geometry>
				<mesh><uri>file://meshes/gripper/finger_right.dae</uri></mesh>
			</geometry>
		</visual>
		<collision name="right_gripper">
			<geometry>
				<mesh><uri>file://meshes/gripper/finger_right.dae</uri></mesh>
			</geometry>
		</collision>
	</link>

	<link name="gripper_left">
		<pose>0 -0.075 1.805 0 -1.57 -3.14</pose>
		<inertial>
			<mass>0.1</mass>
			<inertia>
				<ixx>0.002</ixx>
				<ixy>0</ixy>
				<ixz>0</ixz>
				<iyy>0.0002</iyy>
				<iyz>0</iyz>
				<izz>0.0002</izz>
			</inertia>
		</inertial>
		<visual name="left_gripper">
			<geometry>
				<mesh><uri>file://meshes/gripper/finger_left.dae</uri></mesh>
			</geometry>
		</visual>
		<collision name="left_gripper">
			<geometry>
				<mesh><uri>file://meshes/gripper/finger_left.dae</uri></mesh>
			</geometry>
		</collision>
	</link>

	<joint name="base" type="revolute">
		<pose>0 0 0 0 0 0</pose>
		<child>base</child>
		<parent>world</parent>
		<axis>
			<limit>
				<lower>-2</lower>
				<upper>2</upper>
			</limit>
			<xyz>0 0 1</xyz>
		</axis>
	</joint>

	<joint name="joint1" type="revolute">
		<pose>0 0 0 0 0 0</pose>
		<child>joint1</child>
		<parent>base</parent>
		<axis>
			<limit>
				<lower>-2</lower>
				<upper>2</upper>
			</limit>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>

	<joint name="link1-joint1" type="fixed">
		<pose>0 0 -0.05 0 0 0</pose>
		<child>link1</child>
		<parent>joint1</parent>
		<axis>
			<limit>
				<lower>0</lower>
				<upper>0</upper>
			</limit>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>

	<joint name="joint2" type="revolute">
		<pose>0 0 -0.05 0 0 0</pose>
		<child>joint2</child>
		<parent>link1</parent>
		<axis>
			<limit>
				<lower>-2</lower>
				<upper>2</upper>
			</limit>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>

	<joint name="link2-joint2" type="fixed">
		<pose>0 0 -0.05 0 0 0</pose>
		<child>link2</child>
		<parent>joint2</parent>
		<axis>
			<limit>
				<lower>0</lower>
				<upper>0</upper>
			</limit>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>

	<joint name="gripper_left" type="revolute">
		<pose>0 0 -0.05 0 0 0</pose>
		<child>gripper_left</child>
		<parent>gripperbase</parent>
		<axis>
			<limit>
				<lower>0</lower>
				<upper>0</upper>
			</limit>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>
	<joint name="gripper_right" type="revolute">
		<pose>0 0 -0.05 0 0 0</pose>
		<child>gripper_right</child>
		<parent>gripperbase</parent>
		<axis>
			<limit>
				<lower>0</lower>
				<upper>0</upper>
			</limit>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>
	  
	<joint name="gripperbase" type="fixed">
		<pose>0 0 -0.05 0 0 0</pose>
		<parent>link2</parent>
		<child>gripperbase</child>
		<axis>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>

	<joint name="gripper_middle" type="fixed">
		<pose>0 0 0 0 0 0</pose>
		<parent>gripperbase</parent>
		<child>gripper_middle</child>
		<axis>
			<xyz>0 1 0</xyz>
		</axis>
	</joint>
	 
